{
    "name": "mangaonlineviewer",
    "description": "Userscript that helps to read Manga faster by loading all the pages at once",
    "version": "30.0.0",
    "author": "TagoDR",
    "license": "MIT",
    "type": "module",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TagoDR/MangaOnlineViewer.git"
    },
    "keywords": [
        "manga",
        "reader",
        "viewer",
        "online",
        "loader"
    ],
    "bugs": "https://github.com/TagoDR/MangaOnlineViewer/issues",
    "homepage": "https://github.com/TagoDR/MangaOnlineViewer#readme",
    "scripts": {
        "start": "vite",
        "build": "gulp build",
        "release": "gulp release",
        "serve": "vite preview"
    },
    "devDependencies": {
        "@fal-works/esbuild-plugin-global-externals": "^2.1.2",
        "@rbnlffl/rollup-plugin-eslint": "^3.0.0",
        "@rollup/plugin-commonjs": "^22.0.1",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@rollup/plugin-typescript": "^8.3.3",
        "@types/file-saver": "^2.0.5",
        "@types/imagesloaded": "^4.1.2",
        "@types/jquery": "^3.5.14",
        "@types/lodash": "^4.14.182",
        "@types/node": "^18.0.0",
        "@types/nprogress": "^0.2.0",
        "@types/react": "^18.0.13",
        "@types/react-dom": "^18.0.5",
        "@types/styled-components": "^5.1.25",
        "@types/tampermonkey": "^4.0.5",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "@vitejs/plugin-react": "^1.3.0",
        "del": "^6.1.1",
        "esbuild": "^0.14.47",
        "esbuild-minify-templates": "^0.8.0",
        "eslint": "^8.17.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "gulp": "^4.0.2",
        "gulp-preprocess": "^4.0.2",
        "prettier": "^2.7.1",
        "rollup": "^2.75.7",
        "rollup-plugin-bundle-size": "^1.0.3",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-external-globals": "^0.6.1",
        "ts-import": "4.0.0-beta.2",
        "tslib": "^2.4.0",
        "typescript": "^4.7.4",
        "userscript-meta": "^1.0.1",
        "vite": "^2.9.9",
        "vite-plugin-monkey": "^0.2.13"
    },
    "dependencies": {
        "@mantine/core": "^4.2.9",
        "@mantine/hooks": "^4.2.7",
        "color-scheme": "^1.0.1",
        "file-saver": "^2.0.5",
        "imagesloaded": "^5.0.0",
        "jquery": "^3.6.0",
        "jszip": "^3.10.0",
        "lodash": "^4.17.21",
        "nprogress": "^0.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "recoil": "^0.7.3",
        "styled-components": "^5.3.5",
        "sweetalert2": "^11.4.19",
        "tabler-icons-react": "^1.50.0"
    },
    "browserslist": {
        "production": [
            ">20.0%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "prettier": {
        "semi": true,
        "singleQuote": true,
        "trailingComma": "all",
        "printWidth": 100
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "ecmaVersion": "latest",
            "project": [
                "./tsconfig.json",
                "./tsconfig.node.json"
            ]
        },
        "env": {
            "browser": true,
            "es6": true,
            "greasemonkey": true
        },
        "globals": {
            "W": "readonly",
            "$": "readonly",
            "JSZip": "readonly",
            "NProgress": "readonly",
            "jscolor": "readonly",
            "ColorScheme": "readonly",
            "Swal": "readonly"
        },
        "extends": [
            "airbnb",
            "airbnb-typescript",
            "prettier"
        ],
        "plugins": [
            "react",
            "jsx-a11y",
            "import",
            "prettier",
            "@typescript-eslint"
        ],
        "settings": {
            "import/resolver": {
                "node": {
                    "extensions": [
                        ".js",
                        ".ts",
                        ".jsx",
                        ".tsx",
                        ".json"
                    ]
                }
            },
            "import/extensions": [
                ".js",
                ".ts",
                ".mjs",
                ".jsx",
                ".tsx"
            ]
        },
        "rules": {
            "prettier/prettier": [
                "warn"
            ],
            "no-console": "warn",
            "import/extensions": [
                "error",
                "always",
                {
                    "pattern": {
                        "jsx": "never",
                        "tsx": "never",
                        "ts": "never",
                        "js": "never"
                    }
                }
            ],
            "react/function-component-definition": [
                2,
                {
                    "namedComponents": "arrow-function",
                    "unnamedComponents": "arrow-function"
                }
            ]
        }
    },
    "eslintIgnore": [
        "node_modules",
        "*.min.js",
        "dist/**",
        "gulpfile*.js",
        "Manga_OnlineViewer.meta.js",
        "Manga_OnlineViewer.user.js",
        "Manga_OnlineViewer_Adult.meta.js",
        "Manga_OnlineViewer_Adult.user.js"
    ]
}
